<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scheduler.viewsAndControllers.Confirm">
   <children>
      <Label fx:id="ConfirmAction" alignment="CENTER" layoutX="46.0" layoutY="59.0" prefHeight="25.0" prefWidth="258.0" text="Do you wish to complete this operation?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="OK" layoutX="58.0" layoutY="104.0" mnemonicParsing="false" onAction="#confirmButtonListener" text="confirm" />
      <Button fx:id="Cancel" layoutX="244.0" layoutY="104.0" mnemonicParsing="false" onAction="#cancelButtonListener" text="cancel" />
   </children>
</AnchorPane>
